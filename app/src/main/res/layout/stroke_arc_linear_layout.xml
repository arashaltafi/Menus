<?xml version="1.0" encoding="utf-8"?>
<com.arash.altafi.menus.bottom_sheet.bottomSheet6.arcview.ArcLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    app:itemsOffset="7dp"
    app:useMinPadding="true">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/EditActivityImageView"
        android:padding="3dp"
        android:src="@drawable/ic_stroke_blue" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/include_buttons_text_stroke_color"
        style="@style/EditActivityImageView"
        android:src="@drawable/ic_color" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/include_buttons_text_stroke_width"
        style="@style/EditActivityImageView"
        android:src="@drawable/ic_bi_direction_arrow" />

</com.arash.altafi.menus.bottom_sheet.bottomSheet6.arcview.ArcLinearLayout>
